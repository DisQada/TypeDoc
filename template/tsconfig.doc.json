{
  "include": ["../../../../{{types}}/"],
  "exclude": ["**/node_modules/**/*"],
  "compilerOptions": {
    "noEmitOnError": false,

    "declaration": true,
    "forceConsistentCasingInFileNames": true,

    "target": "ESNext",
    "module": "NodeNext",
    "moduleDetection": "auto",
    "moduleResolution": "NodeNext"
  }
}
