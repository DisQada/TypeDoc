{
    "displayName": "TypeDoc",
    "name": "@disqada/typedoc",
    "version": "1.1.0",
    "description": "Generate types and documentation with a click",
    "scripts": {
        "config": "node ./src/config.js",
        "clean": "node ./src/index.js",
        "build": "tsc -p ./config/tsconfig.json && npm run clean",
        "doc": "typedoc --options ./config/typedoc.json",
        "lint": "eslint . --fix",
        "lint": "eslint . --fix --ignore-path .gitignore",
        "format": "prettier . --write --ignore-path .gitignore",
        "test": "npx jest"
    },
    "type": "commonjs",
    "files": [
        "src/",
        "config/",
        "CHANGELOG.md",
        "README.md",
        "LICENSE"
    ],
    "homepage": "https://disqada.github.io/TypeDoc",
    "repository": {
        "type": "git",
        "url": "https://github.com/DisQada/TypeDoc.git"
    },
    "bugs": {
        "url": "https://github.com/DisQada/TypeDoc/issues"
    },
    "keywords": [
        "Types",
        "JSDoc",
        "TypeDoc",
        "Documentation",
        "Auto Generation"
    ],
    "author": {
        "name": "The Alpha",
        "email": "contact@nabilalsaiad.com",
        "url": "https://links.nabilalsaiad.com"
    },
    "contributors": [],
    "license": "Apache-2.0",
    "peerDependencies": {
        "@mxssfd/typedoc-theme": "1.1.x",
        "typedoc": "0.25.x",
        "typedoc-plugin-extras": "3.0.x",
        "typedoc-plugin-missing-exports": "2.1.x",
        "typescript": "5.2.x"
    },
    "devDependencies": {
        "@types/jest": "*",
        "eslint": "*",
        "eslint-plugin-import": "*",
        "eslint-plugin-jest": "*",
        "eslint-plugin-jsdoc": "*",
        "eslint-plugin-promise": "*",
        "prettier": "*"
    }
}
